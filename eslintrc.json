{
  "extends": [
    "plugin:react/recommended",
    "standard",
    "plugin:prettier/recommended",
    "shared-config"
  ],
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}
